            <Font name="Luminari" size="24.0" />
         </font>
      </Label>
      <Button layoutX="47.0" layoutY="107.0" mnemonicParsing="false" onAction="#todayChange" style="-fx-background-color: white;" stylesheets="@Style.css" text="Today" />
      <Button layoutX="36.0" layoutY="160.0" mnemonicParsing="false" onAction="#tmrwChange" style="-fx-background-color: white;" text="Tomorrow" />
      <Button layoutX="30.0" layoutY="218.0" mnemonicParsing="false" onAction="#changeSign" style="-fx-background-color: white;" text="Change sign" />
